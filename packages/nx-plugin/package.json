{
  "name": "@eternagame/nx-plugin",
  "description": "Eterna standard Nx setup and utilities",
  "version": "2.0.0-dev",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/eternagame/workspace-helpers.git",
    "directory": "packages/nx-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "files": [
    "dist",
    "preset.json",
    "generators.json"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist/*.js": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    },
    "./*.json": "./*.json"
  },
  "generators": "./generators.json",
  "dependencies": {
    "@nrwl/devkit": "^14.6.5",
    "yaml": "^2.1.3"
  },
  "peerDependencies": {
    "nx": "^14.6.5"
  },
  "nx": {
    "implicitDependencies": [
      "vite-utils"
    ]
  }
}
