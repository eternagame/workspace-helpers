{
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "editor.codeActionsOnSave": [
    "source.fixAll.eslint"
  ],
  "eslint.workingDirectories": [{"mode": "auto"}],
  "jest.jestCommandLine": "npm run test -ws --if-present -- "
}
