{
  "$schema": "http://json-schema.org/schema",
  "name": "eternagame/nx",
  "version": "1.0.0",
  "generators": {
    "preset": {
      "factory": "./dist/generators/preset",
      "schema": "./dist/generators/preset/schema.json",
      "description": "Initializes an Nx project"
    },
    "package": {
      "factory": "./dist/generators/package",
      "schema": "./dist/generators/package/schema.json",
      "description": "Adds an empty generic package to an Nx project"
    },
    "lib-ts": {
      "factory": "./dist/generators/lib-ts",
      "schema": "./dist/generators/lib-ts/schema.json",
      "description": "Adds new TypeScript library to an Nx project"
    },
    "app-node": {
      "factory": "./dist/generators/app-node",
      "schema": "./dist/generators/app-node/schema.json",
      "description": "Adds a new Typescript NodeJS application to an Nx project"
    },
    "app-web": {
      "factory": "./dist/generators/app-web",
      "schema": "./dist/generators/app-web/schema.json",
      "description": "Adds a new Typescript browser application to an Nx project"
    },
    "app-vue": {
      "factory": "./dist/generators/app-vue",
      "schema": "./dist/generators/app-vue/schema.json",
      "description": "Adds a new Vue browser application to an Nx project"
    },
    "license": {
      "factory": "./dist/generators/license",
      "schema": "./dist/generators/license/schema.json",
      "description": "Re-licenses an Nx project"
    }
  }
}
