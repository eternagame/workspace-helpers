{
  "$schema": "http://json-schema.org/schema",
  "name": "eternagame/nx-plugin",
  "version": "1.0.0",
  "generators": {
    "preset": {
      "factory": "./dist/src/generators/preset",
      "schema": "./dist/src/generators/preset/schema.json",
      "description": "Initializes an Nx-managed repository"
    },
    "package": {
      "factory": "./dist/src/generators/package/base",
      "schema": "./dist/src/generators/package/base/schema.json",
      "description": "Adds an empty generic package"
    },
    "ts-lib": {
      "factory": "./dist/src/generators/package/ts/lib",
      "schema": "./dist/src/generators/package/ts/lib/schema.json",
      "description": "Adds a new isomorphic TypeScript library"
    },
    "node-lib": {
      "factory": "./dist/src/generators/package/node/lib",
      "schema": "./dist/src/generators/package/node/lib/schema.json",
      "description": "Adds a new Typescript NodeJS library"
    },
    "node-app": {
      "factory": "./dist/src/generators/package/node/app",
      "schema": "./dist/src/generators/package/node/app/schema.json",
      "description": "Adds a new Typescript NodeJS application"
    },
    "web-lib": {
      "factory": "./dist/src/generators/package/web/lib",
      "schema": "./dist/src/generators/package/web/lib/schema.json",
      "description": "Adds a new Typescript browser library"
    },
    "web-app": {
      "factory": "./dist/src/generators/package/web/app",
      "schema": "./dist/src/generators/package/web/app/schema.json",
      "description": "Adds a new Typescript browser application"
    },
    "vue-lib": {
      "factory": "./dist/src/generators/package/vue/lib",
      "schema": "./dist/src/generators/package/vue/lib/schema.json",
      "description": "Adds a new Vue browser library"
    },
    "vue-app": {
      "factory": "./dist/src/generators/package/vue/app",
      "schema": "./dist/src/generators/package/vue/app/schema.json",
      "description": "Adds a new Vue browser application"
    },
    "license": {
      "factory": "./dist/src/generators/license/repo",
      "schema": "./dist/src/generators/license/repo/schema.json",
      "description": "Re-licenses the repository and its packages"
    },
    "license-package": {
      "factory": "./dist/src/generators/license/package",
      "schema": "./dist/src/generators/license/package/schema.json",
      "description": "Re-licenses a package"
    },
    "release": {
      "factory": "./dist/src/generators/release/repo",
      "schema": "./dist/src/generators/release/repo/schema.json",
      "description": "Sets up automated releases"
    },
    "release-package": {
      "factory": "./dist/src/generators/release/package",
      "schema": "./dist/src/generators/release/package/schema.json",
      "description": "Sets up automated releases for a package"
    },
    "cypress-e2e": {
      "factory": "./dist/src/generators/package/cypress/e2e",
      "schema": "./dist/src/generators/package/cypress/e2e/schema.json",
      "description": "Adds a new Cypress end to end testing package"
    },
    "cypress-ct": {
      "factory": "./dist/src/generators/package/cypress/ct",
      "schema": "./dist/src/generators/package/cypress/ct/schema.json",
      "description": "Adds Cypress component testing to an existing package"
    }
  }
}
