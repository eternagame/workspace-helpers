{
  "$schema": "http://json-schema.org/schema",
  "name": "eternagame/nx",
  "version": "1.0.0",
  "generators": {
    "preset": {
      "factory": "./dist/generators/preset",
      "schema": "./dist/generators/preset/schema.json",
      "description": "Initializes an Nx project"
    },
    "package": {
      "factory": "./dist/generators/package/base",
      "schema": "./dist/generators/package/base/schema.json",
      "description": "Adds an empty generic package to an Nx project"
    },
    "ts-lib": {
      "factory": "./dist/generators/package/ts/lib",
      "schema": "./dist/generators/package/ts/lib/schema.json",
      "description": "Adds new isomorphic TypeScript library to an Nx project"
    },
    "node-lib": {
      "factory": "./dist/generators/package/node/lib",
      "schema": "./dist/generators/package/node/lib/schema.json",
      "description": "Adds a new Typescript NodeJS library to an Nx project"
    },
    "node-app": {
      "factory": "./dist/generators/package/node/app",
      "schema": "./dist/generators/package/node/app/schema.json",
      "description": "Adds a new Typescript NodeJS application to an Nx project"
    },
    "web-lib": {
      "factory": "./dist/generators/package/web/lib",
      "schema": "./dist/generators/package/web/lib/schema.json",
      "description": "Adds a new Typescript browser library to an Nx project"
    },
    "web-app": {
      "factory": "./dist/generators/package/web/app",
      "schema": "./dist/generators/package/web/app/schema.json",
      "description": "Adds a new Typescript browser application to an Nx project"
    },
    "vue-app": {
      "factory": "./dist/generators/package/vue/app",
      "schema": "./dist/generators/package/vue/app/schema.json",
      "description": "Adds a new Vue browser application to an Nx project"
    },
    "license": {
      "factory": "./dist/generators/license/repo",
      "schema": "./dist/generators/license/repo/schema.json",
      "description": "Re-licenses an Nx project"
    },
    "license-package": {
      "factory": "./dist/generators/license/package",
      "schema": "./dist/generators/license/package/schema.json",
      "description": "Re-licenses a package"
    },
    "release": {
      "factory": "./dist/generators/release/repo",
      "schema": "./dist/generators/release/repo/schema.json",
      "description": "Sets up automated releases"
    },
    "release-package": {
      "factory": "./dist/generators/release/package",
      "schema": "./dist/generators/release/package/schema.json",
      "description": "Sets up automated releases for a package"
    }
  }
}
