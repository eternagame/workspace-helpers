{
  "$schema": "http://json-schema.org/schema",
  "name": "eternagame/nx",
  "version": "1.0.0",
  "generators": {
    "preset": {
      "factory": "./dist/generators/preset/preset",
      "schema": "./dist/generators/preset/schema.json",
      "description": "Initializes an Nx workspace"
    },
    "preset-eterna": {
      "factory": "./dist/generators/preset-eterna/preset-eterna",
      "schema": "./dist/generators/preset-eterna/schema.json",
      "description": "Initializes an Nx workspace for use as part of the Eterna project"
    },
    "package": {
      "factory": "./dist/generators/package/package",
      "schema": "./dist/generators/package/schema.json",
      "description": "Initializes an Nx workspace"
    },
    "license": {
      "factory": "./dist/generators/license/license",
      "schema": "./dist/generators/license/schema.json",
      "description": "Re-licenses an Nx workspace"
    }
  }
}
